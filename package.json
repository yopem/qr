{
  "name": "yopem-qr",
  "private": true,
  "type": "module",
  "version": "0.0.1",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "bun run --bun vite dev",
    "build": "bun run --bun vite build",
    "preview": "bun run --bun vite preview",
    "start": "bun run --bun vite preview",
    "prepare": "bun run --bun svelte-kit sync",
    "db:check": "bun run --bun drizzle-kit check",
    "db:drop": "bun run --bun drizzle-kit drop",
    "db:generate": "bun run --bun drizzle-kit generate",
    "db:migrate": "bun run --bun drizzle-kit migrate",
    "db:pull": "bun run --bun drizzle-kit introspect",
    "db:push": "bun run --bun drizzle-kit push",
    "db:studio": "bun run --bun drizzle-kit studio",
    "db:up": "bun run --bun drizzle-kit up",
    "check": "bun run --bun svelte-kit sync && bun run --bun svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun run --bun svelte-kit sync && bun run --bun svelte-check --tsconfig ./tsconfig.json --watch",
    "format:check": "bun run --bun prettier --check \"**/*.{svelte,ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "format:write": "bun run --bun prettier --write \"**/*.{svelte,ts,tsx,mts,js,jsx,mjs,json,md}\"",
    "lint": "bun run --bun eslint ."
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@eslint/js": "^9.39.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.561.0",
    "@openauthjs/openauth": "^0.4.3",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.48.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@types/node": "^25",
    "@types/qrcode": "^1.5.6",
    "bits-ui": "^2.14.4",
    "bun": "^1.3.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "jsqr": "^1.4.0",
    "lefthook": "^2.0.12",
    "mode-watcher": "^1.1.0",
    "nanoid": "^5.1.6",
    "postgres": "^3.4.7",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "qrcode": "^1.5.4",
    "sk-seo": "^0.5.4",
    "svelte": "^5.43.8",
    "svelte-check": "^4.3.4",
    "svelte-sonner": "^1.0.5",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "zod": "^4.1.13"
  },
  "packageManager": "bun@1.3.3"
}
